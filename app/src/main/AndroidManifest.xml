<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hitherejoe.animate">

    <application
        android:name=".AnimateApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.ActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.TouchFeedBackActivity"
            android:label="@string/label_touch_feedback"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".ui.activity.AnimatedVectorDrawablesActivity"
            android:label="@string/label_animated_vector_drawables"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".ui.activity.CircularRevealActivity"
            android:label="@string/label_circular_reveal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.activity.InterpolatorActivity"
            android:label="@string/label_interpolators"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".ui.activity.ObjectAnimatorActivity"
            android:label="@string/label_object_animator"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".ui.activity.PopupActivity"
            android:theme="@style/TranslucentActivity" />
        <activity
            android:name=".ui.activity.SharedTransitionInToolbarActivity"
            android:label="@string/label_shared_element_transitions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.activity.SharedTransitionsInActionbarActivity"
            android:label="@string/label_shared_element_transitions"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".ui.activity.SharedTransitionsActivity"
            android:label="@string/label_shared_content_transitions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.activity.SharedTransitionToolbarActivity"
            android:label="@string/label_shared_element_transitions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.activity.TransitionInActivity"
            android:label="@string/label_window_content_transitions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.activity.MorphTransitionsActivity"
            android:label="@string/label_morph_transitions"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".ui.activity.ViewPropertyAnimatorActivity"
            android:label="@string/label_view_property_animator"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".ui.activity.WindowTransitionsActivity"
            android:label="@string/label_window_content_transitions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.activity.WindowTransitionsActivityExplode"
            android:label="@string/label_explode_window_content_transitions"
            android:theme="@style/AppTheme.ExplodeContent" />
        <activity
            android:name=".ui.activity.ScenesTransitionActivity"
            android:label="Scenes Transition"
            android:theme="@style/AppTheme.ActionBar" />
        <activity android:name=".ui.activity.ViewPageTransformerActivity" />
        <activity android:name=".ui.activity.CardFlipActivity" />
        <activity android:name=".ui.activity.ZoomActivity" />
        <activity android:name=".ui.activity.LayoutTransitionActivity"
                android:theme="@style/AppTheme.ActionBar">
        </activity>
    </application>

</manifest>